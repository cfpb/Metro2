{
  "name": "front-end",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "build": "vite build --base=/static/",
    "dev": "vite",
    "preview": "vite preview",
    "preview:test": "start-server-and-test preview http://localhost:4173",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:e2e": "yarn cypress open",
    "test:e2e:headless": "yarn cypress run",
    "test:e2e:ci": "yarn cypress run --record",
    "test:e2e:dev": "yarn cypress open",
    "format": "prettier -uw --cache --ignore-path .gitignore .",
    "run-tsc": "tsc",
    "run-eslint": "eslint --cache --fix --ignore-path .gitignore --ext .ts,.tsx .",
    "run-stylelint": "stylelint --cache --fix --ignore-path .gitignore **/*.less",
    "lint": "run-p run-tsc run-eslint run-stylelint",
    "validate": "run-p lint test:ci test:e2e:headless"
  },
  "dependencies": {
    "@cfpb/cfpb-design-system": "0.26.1",
    "@tanstack/react-query": "^5.80.6",
    "@tanstack/react-router": "^1.120.20",
    "@tanstack/router-devtools": "^1.17.4",
    "@tanstack/router-zod-adapter": "^1.81.5",
    "@tanstack/zod-adapter": "^1.99.9",
    "ag-grid-community": "^33.3.0",
    "ag-grid-react": "^33.3.0",
    "design-system-react": "https://github.com/cfpb/design-system-react.git",
    "exceljs": "^4.4.0",
    "query-string": "^9.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.14.4",
    "@nabla/vite-plugin-eslint": "^2.0.4",
    "@testing-library/cypress": "^10.0.3",
    "@testing-library/dom": "9.3.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/css-mediaquery": "0.1.1",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@types/react-router-dom": "5.3.3",
    "@types/testing-library__jest-dom": "5.14.5",
    "@types/wicg-file-system-access": "^2023.10.5",
    "@typescript-eslint/eslint-plugin": "5.59.6",
    "@typescript-eslint/parser": "5.59.6",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-istanbul": "0.31.1",
    "autoprefixer": "10.4.14",
    "css-mediaquery": "0.1.2",
    "cypress": "^13.13.2",
    "cypress-real-events": "^1.13.0",
    "eslint": "^8.45.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-cypress": "2.13.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-prefer-function-component": "3.1.0",
    "eslint-plugin-testing-library": "5.11.0",
    "eslint-plugin-unicorn": "47.0.0",
    "jsdom": "22.0.0",
    "less": "4.1.3",
    "lint-staged": "13.2.2",
    "msw": "1.2.1",
    "npm-run-all": "4.1.5",
    "postcss": "^8.4.38",
    "postcss-less": "^6.0.0",
    "prettier": "2.8.8",
    "start-server-and-test": "2.0.0",
    "stylelint": "15.6.1",
    "stylelint-config-recommended-less": "1.0.4",
    "stylelint-config-standard": "33.0.0",
    "typescript": "5.0.4",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "0.31.1",
    "whatwg-fetch": "3.6.2"
  },
  "browserslist": {
    "production": "Edge >= 18, Firefox >= 60, Chrome >= 61, Safari >= 11, Opera >= 48",
    "development": [
      "last 1 chrome version",
      "last 1 firefox version"
    ]
  },
  "lint-staged": {
    "*": "prettier -uw --cache",
    "*.css": "stylelint --cache --fix",
    "*.{ts,tsx}": [
      "eslint --cache --fix",
      "vitest related --run --coverage=false"
    ]
  },
  "packageManager": "yarn@4.1.1+sha256.f3cc0eda8e5560e529c7147565b30faa43b4e472d90e8634d7134a37c7f59781"
}
