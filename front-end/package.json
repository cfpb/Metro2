{
  "name": "front-end",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "format": "prettier --write \"**/*.{js,jsx,json,md,css,less,cjs}\"",
    "run-eslint": "eslint . --ext .js,.jsx",
    "run-stylelint": "stylelint 'src/**/*.{css,less}' --fix",
    "lint": "npm-run-all run-eslint run-stylelint",
    "test:e2e": "start-server-and-test dev http://localhost:3000 'yarn cypress open --e2e --browser chrome'",
    "test:component": "start-server-and-test dev http://localhost:3000 'yarn cypress open --component --browser chrome'",
    "test:headless-e2e": "start-server-and-test dev http://localhost:3000 'yarn cypress run --e2e'",
    "test:headless-component": "start-server-and-test dev http://localhost:3000 'yarn cypress run --component'",
    "test:unit": "vitest run",
    "test": "npm-run-all test:headless-e2e test:headless-component test:unit"
  },
  "dependencies": {
    "@cfpb/cfpb-design-system": "0.20.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "cypress": "12.4.1",
    "eslint": "8.32.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-prefer-function-component": "3.1.0",
    "husky": "8.0.3",
    "jsdom": "21.1.0",
    "less": "4.1.3",
    "lint-staged": "13.1.0",
    "npm-run-all": "4.1.5",
    "postcss-less": "6.0.0",
    "prettier": "2.8.3",
    "start-server-and-test": "1.15.3",
    "stylelint": "14.16.1",
    "stylelint-config-prettier": "9.0.4",
    "stylelint-config-recommended-less": "1.0.4",
    "stylelint-config-standard": "29.0.0",
    "vite": "^4.0.0",
    "vitest": "0.28.3"
  }
}
